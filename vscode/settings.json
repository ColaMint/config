{
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Hack Nerd Font",
  "editor.fontLigatures": false,
  "editor.fontWeight": "normal",
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.insertMode": "replace",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.wordBasedSuggestions": "allDocuments",
  "editor.wordWrap": "on",
  "files.autoGuessEncoding": true,
  "godot_tools.gdscript_lsp_server_port": 6005,
  "projectManager.git.baseFolders": [
    "/Users/liming/go/src/github.com/ColaMint",
    "/Users/liming/go/src/gitlab.umcasual.cn"
  ],
  "docomment.advanced": {
    "cs": {
      "namespace": {
        "attributes": ["summary"]
      },
      "class": {
        "attributes": ["summary", "typeparam"]
      },
      "interface": {
        "attributes": ["summary", "typeparam"]
      },
      "struct": {
        "attributes": ["summary"]
      },
      "enum": {
        "attributes": ["summary"]
      },
      "delegate": {
        "attributes": ["summary", "param", "typeparam", "returns"]
      },
      "field": {
        "attributes": ["summary"]
      },
      "property": {
        //"attributes": ["summary", "value"]
        "attributes": ["summary"]
      },
      "method": {
        "attributes": ["summary", "param", "typeparam", "returns"]
      },
      "event": {
        "attributes": ["summary"]
      }
    }
  },
  "projectManager.sortList": "Recent",
  "sortJSON.orderOverride": ["name", "version", "description"],
  "sortJSON.orderUnderride": ["dependencies", "devDependencies"],
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindings": [
    {
      "before": ["d", "h"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["d", "j"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["d", "k"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "after": ["<C-d>"],
      "before": ["J"]
    },
    {
      "after": ["<C-u>"],
      "before": ["K"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    }
  ],
  "vim.useSystemClipboard": false,
  "workbench.settings.editor": "json",
  "zenMode.hideLineNumbers": false,
  "workbench.startupEditor": "none",
  "godotTools.lsp.serverPort": 6005,
  "godotTools.editorPath.godot4": "/Applications/Godot_mono.app/Contents/MacOS/Godot",
  "go.toolsManagement.autoUpdate": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.lightIndicatorStyleLineWidth": 2,
  "redhat.telemetry.enabled": true,
  "debug.onTaskErrors": "abort",
  "Codegeex.Privacy": true,
  "Codegeex.EnableExtension": false,
  "editor.accessibilitySupport": "off",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "cmake.showOptionsMovedNotification": false,
  "dart.openDevTools": "flutter",
  "vetur.validation.script": false
}
